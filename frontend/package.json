{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "i18n-report": "vue-i18n-extract report --vueFiles './src/**/*.?(ts|vue)' --languageFiles './src/i18n/*.?(json|yml|yaml|js)'",
    "i18n-add": "vue-i18n-extract report --add --exclude types --vueFiles './src/**/*.?(ts|vue)' --languageFiles './src/i18n/*.?(json|yml|yaml|js)'",
    "i18n-remove": "vue-i18n-extract report --remove --exclude types --vueFiles './src/**/*.?(ts|vue)' --languageFiles './src/i18n/*.?(json|yml|yaml|js)'",
    "devtools": "./node_modules/.bin/vue-devtools"
  },
  "dependencies": {
    "@formkit/tempo": "^0.1.2",
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.1.5",
    "@types/deep-equal": "^1.0.4",
    "@types/lodash": "^4.17.12",
    "@vee-validate/zod": "^4.14.6",
    "dayjs": "^1.11.13",
    "deep-equal": "^2.2.3",
    "lodash": "^4.17.21",
    "vee-validate": "^4.14.6",
    "vue": "^3.5.12",
    "vue-i18n": "^9.14.1",
    "vue-router": "^4.4.5",
    "vue-tailwind-datepicker": "^1.7.3",
    "vue-toastification": "2.0.0-rc.5",
    "yup": "^1.4.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/types": "^7.26.0",
    "@tailwindcss/typography": "^0.5.15",
    "@types/node": "^22.8.1",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/devtools": "^7.5.4",
    "autoprefixer": "^10.4.20",
    "daisyui": "^4.12.13",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vue-i18n-extract": "^2.0.7",
    "vue-tsc": "^2.1.8"
  }
}
