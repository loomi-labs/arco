{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "i18n-report": "vue-i18n-extract report --vueFiles './src/**/*.?(ts|vue)' --languageFiles './src/i18n/*.?(json|yml|yaml|js)'",
    "i18n-add": "vue-i18n-extract report --add --exclude types --vueFiles './src/**/*.?(ts|vue)' --languageFiles './src/i18n/*.?(json|yml|yaml|js)'",
    "i18n-remove": "vue-i18n-extract report --remove --exclude types --vueFiles './src/**/*.?(ts|vue)' --languageFiles './src/i18n/*.?(json|yml|yaml|js)'",
    "devtools": "./node_modules/.bin/vue-devtools"
  },
  "dependencies": {
    "@formkit/tempo": "^0.1.2",
    "@heroicons/vue": "^2.1.5",
    "@types/deep-equal": "^1.0.4",
    "@types/lodash": "^4.17.10",
    "@vee-validate/zod": "^4.13.2",
    "deep-equal": "^2.2.3",
    "lodash": "^4.17.21",
    "vee-validate": "^4.13.2",
    "vue": "^3.5.11",
    "vue-i18n": "^9.14.1",
    "vue-router": "^4.4.5",
    "vue-toastification": "2.0.0-rc.5",
    "yup": "^1.4.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/types": "^7.25.7",
    "@types/node": "^22.7.5",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/devtools": "^7.4.6",
    "autoprefixer": "^10.4.20",
    "daisyui": "^4.12.13",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.3",
    "vite": "^5.4.8",
    "vue-i18n-extract": "^2.0.7",
    "vue-tsc": "^2.1.6"
  }
}
