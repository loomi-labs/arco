{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "check": "vue-tsc --noEmit",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "i18n-report": "vue-i18n-extract report --vueFiles './src/**/*.?(ts|vue)' --languageFiles './src/i18n/*.?(json|yml|yaml|js)'",
    "i18n-add": "vue-i18n-extract report --add --exclude types --vueFiles './src/**/*.?(ts|vue)' --languageFiles './src/i18n/*.?(json|yml|yaml|js)'",
    "i18n-remove": "vue-i18n-extract report --remove --exclude types --vueFiles './src/**/*.?(ts|vue)' --languageFiles './src/i18n/*.?(json|yml|yaml|js)'",
    "devtools": "./node_modules/.bin/vue-devtools"
  },
  "dependencies": {
    "@formkit/tempo": "^0.1.2",
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.2.0",
    "@types/deep-equal": "^1.0.4",
    "@types/lodash": "^4.17.15",
    "@vee-validate/zod": "^4.15.0",
    "@vueuse/core": "^12.7.0",
    "dayjs": "^1.11.13",
    "deep-equal": "^2.2.3",
    "lodash": "^4.17.21",
    "vee-validate": "^4.15.0",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.1",
    "vue-router": "^4.5.0",
    "vue-tailwind-datepicker": "^1.7.3",
    "vue-toastification": "2.0.0-rc.5",
    "vue3-lottie": "^3.3.1",
    "yup": "^1.6.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@babel/types": "^7.26.9",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.0.8",
    "@types/node": "^22.13.5",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/devtools": "^7.7.2",
    "autoprefixer": "^10.4.20",
    "daisyui": "5.0.0-beta.8",
    "tailwindcss": "^4.0.8",
    "typescript": "^5.7.3",
    "vite": "^6.1.1",
    "vue-i18n-extract": "^2.0.7",
    "vue-tsc": "^2.2.2"
  }
}
