{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "i18n-report": "vue-i18n-extract report --vueFiles './src/**/*.?(ts|vue)' --languageFiles './src/i18n/*.?(json|yml|yaml|js)'",
    "i18n-add": "vue-i18n-extract report --add --exclude types --vueFiles './src/**/*.?(ts|vue)' --languageFiles './src/i18n/*.?(json|yml|yaml|js)'",
    "i18n-remove": "vue-i18n-extract report --remove --exclude types --vueFiles './src/**/*.?(ts|vue)' --languageFiles './src/i18n/*.?(json|yml|yaml|js)'"
  },
  "dependencies": {
    "@formkit/tempo": "^0.1.2",
    "@heroicons/vue": "^2.1.5",
    "@vee-validate/zod": "^4.13.2",
    "vee-validate": "^4.13.2",
    "vue": "^3.5.0",
    "vue-i18n": "^9.14.0",
    "vue-router": "^4.4.3",
    "vue-toastification": "2.0.0-rc.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/types": "^7.25.6",
    "@types/node": "^22.5.2",
    "@vitejs/plugin-vue": "^5.1.3",
    "autoprefixer": "^10.4.20",
    "daisyui": "^4.12.10",
    "postcss": "^8.4.44",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "vite": "^5.4.3",
    "vue-i18n-extract": "^2.0.7",
    "vue-tsc": "^2.1.4"
  }
}
