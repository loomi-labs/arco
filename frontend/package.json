{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "check": "vue-tsc --noEmit && eslint .",
    "build": "vue-tsc && vite build --mode production",
    "build:dev": "vue-tsc && vite build --minify false --mode development",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "i18n-report": "vue-i18n-extract report --vueFiles './src/**/*.?(ts|vue)' --languageFiles './src/i18n/*.?(json|yml|yaml|js)'",
    "i18n-add": "vue-i18n-extract report --add --exclude types --vueFiles './src/**/*.?(ts|vue)' --languageFiles './src/i18n/*.?(json|yml|yaml|js)'",
    "i18n-remove": "vue-i18n-extract report --remove --exclude types --vueFiles './src/**/*.?(ts|vue)' --languageFiles './src/i18n/*.?(json|yml|yaml|js)'",
    "devtools": "./node_modules/.bin/vue-devtools"
  },
  "dependencies": {
    "@formkit/tempo": "^0.1.2",
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.2.0",
    "@types/deep-equal": "^1.0.4",
    "@types/lodash": "^4.17.20",
    "@vee-validate/zod": "^4.15.1",
    "@vueuse/core": "^14.0.0",
    "@wailsio/runtime": "3.0.0-alpha.73",
    "dayjs": "^1.11.19",
    "deep-equal": "^2.2.3",
    "flag-icons": "^7.5.0",
    "lodash": "^4.17.21",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.24",
    "vue-i18n": "^11.1.12",
    "vue-router": "^4.6.3",
    "vue-tailwind-datepicker": "^1.7.4",
    "vue-toastification": "2.0.0-rc.5",
    "vue3-lottie": "^3.3.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@babel/types": "^7.28.5",
    "@eslint/js": "^9.39.1",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.17",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/devtools": "^8.0.3",
    "@vue/eslint-config-typescript": "^14.6.0",
    "autoprefixer": "^10.4.22",
    "daisyui": "5.5.3",
    "eslint": "^9.39.1",
    "eslint-plugin-vue": "^10.5.1",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vite-plugin-eslint": "^1.8.1",
    "vue-eslint-parser": "^10.2.0",
    "vue-i18n-extract": "^2.0.7",
    "vue-tsc": "^3.1.3"
  }
}
